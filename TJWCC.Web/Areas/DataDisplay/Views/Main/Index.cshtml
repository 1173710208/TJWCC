    function getcharttest(liuru) {
        let YSeriesData = [];
        let SSeriesData = [];
        $.loading();
        $.each(liuru[0], function (i, ele) {

            let series = {
                yAxisIndex: i,
                name: liuru[1][i] + "(" + liuru[2][i] + ")",
                type: 'line',
                showSymbol: false,
                //symbolSize: 12,
                data: ele,

            }
            SSeriesData.push(series);
        });
        $.each(liuru[3], function (i, ele) {

            let series = {
                max: ele[i],
                min: liuru[4][i],
                type: 'value',
                show: false,
            }
            YSeriesData.push(series);
        });

        //console.log(SSeriesData);
        //chart.clear();
        chart = echarts.init(document.getElementById("chart"));
        var option = {
            grid: {
                x: '3%',
                y: '3%',
                x2: '5%',
                y2: '12%',
            },
            xAxis: {
                type: 'time',
                //splitNumber: 24
            },
            yAxis: YSeriesData,
            dataZoom: [
                {
                    type: 'slider',//数据滑块
                    start: 50,
                    end: 100,
                    minSpan: 8,    //5min
                    // minSpan:16,   //10min
                    // minSpan:24,   //15min
                    // minSpan:50,   //30min
                    dataBackground: {
                        lineStyle: {
                            color: '#95BC2F'
                        },
                        areaStyle: {
                            color: '#95BC2F',
                            opacity: 1,
                        }
                    },
                    // fillerColor:'rgba(255,255,255,.6)'
                },
                {
                    type: 'inside'//使鼠标在图表中时滚轮可用
                }
            ],
            tooltip: {
                trigger: 'axis',
                formatter: function (params) {
                    var result = params[0].value[0];
                    params.forEach(function (item) {
                        result += '<br/>';
                        result += '<span style="display:inline-block;margin-right:5px;border-radius:10px;width:9px;height:9px;background-color:' + item.color + '"></span>';
                        result += item.seriesName + "：";
                        result += isNaN(item.value[1]) ? 0 : item.value[1];
                    });
                    return result;
                },
            },
            series: SSeriesData
        };

        chart.setOption(option);
        chart.resize();
    }

