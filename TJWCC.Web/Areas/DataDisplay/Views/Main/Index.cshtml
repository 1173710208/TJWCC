                            tbaGraphicslayer.add(graphic1);
                        } else {
                            if (isRemove) {
                                map.removeLayer(graphicslayerR[i]);
                                graphicslayerR[i] = null;
                            }
                            graphicslayerR[i] = new GraphicsLayer();
                            var tmpGraphicR = new esri.Graphic(featureSet.features[i].geometry, ReserviorPolygonSymbol, featureSet.features[i].attributes);
                            //鼠标悬停显示水厂信息
                            dojo.connect(graphicslayerR[i], "onMouseMove", function (evt) {
                                $('#followMouseInfoR').html(this.graphics[0].attributes.LABEL)
                                $('#followMouseInfoR').show().css({
                                    "top": evt.pageY - 20,
                                    "left": evt.pageX + 10
                                });
                            });
                            //鼠标离开隐藏水厂信息
                            dojo.connect(graphicslayerR[i], "onMouseOut", function () {
                                $('#followMouseInfoR').hide();
                            });
                            graphicslayerR[i].add(tmpGraphicR);
                            map.addLayer(graphicslayerR[i]);
                        }
                    }
                });
                var queryTask1 = new esri.tasks.QueryTask(reservoirMapLayer);
                var query1 = new esri.tasks.Query();
                query1.outSpatialReference = map.spatialReference;
                query1.where = "ElementId > 1";
                query1.returnGeometry = true;
                query1.outFields = ["*"];
                queryTask1.execute(query1, function (featureSet) {
                    for (var i = 0; i < featureSet.features.length; i++) {
                        let centerPoint = featureSet.features[i].geometry;
                        let name = featureSet.features[i].attributes.Label;
                        let point = new esri.geometry.Point(centerPoint.x, centerPoint.y, map.spatialReference);
                        // 定义自变量
                        let smsLength = (name.length * 14) + 5;//背景图片宽
                        //let textLevel = smsLength * 49 / 180;       //文字水平偏移
                        var textSymbol1 = new esri.symbol.TextSymbol(name, font1, color1);
                        var sms = new esri.symbol.PictureMarkerSymbol('/Areas/DataDisplay/Content/img/bg-border.png', smsLength, 13).setOffset(0, 4);
                        var graphic1 = new esri.Graphic(point, textSymbol1);
                        var graphic3 = new esri.Graphic(point, sms);
                        tbaGraphicslayer.add(graphic3);
                        tbaGraphicslayer.add(graphic1);
                    }
                });
            }
            map.infoWindow.resize(245, 125);
        });

