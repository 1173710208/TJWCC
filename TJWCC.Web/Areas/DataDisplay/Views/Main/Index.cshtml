    function onclickdelete(node) {
        //console.log(node.id);
        for (let i = 0; i < R4Itemlength; i++) {
            if (R4Id[i] == node.id) {
                R4Id.splice(i, 1);
            }
            if (R4Items[i].id == node.id) {
                R4Items.splice(i, 1);
                R4Itemlength--;
                let right2321c = document.getElementById("right2321c");
                right2321c.innerHTML = '';
                for (let j = 0; j < R4Itemlength; j++) {
                    right2321c.innerHTML += '<div class="right23211" >' + R4Items[j].name + '<button id="' + R4Items[j].id + '" onclick="onclickdelete(this)" class="glyphicon glyphicon-remove-circle right232111"></button></div >';
