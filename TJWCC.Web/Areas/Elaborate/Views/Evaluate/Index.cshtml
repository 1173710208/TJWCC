    function delTable() {
        if (giveScore) {
            $.loading(true);
            if (confirm("您确认要删除吗？") == true) {
                $.ajax({
                    type: "GET",
                    url: "/Elaborate/Evaluate/GetExpertDel?id=" + giveScore,
                    success: function (adata) {
                        clickTab27()
                        $.loading();
                        $.modalMsg("删除成功！", "success");
                    }
                });
            } else {
                return false;
            }

        } else {
            alert("请选择一项删除")
        }
    }
    function confirmTable() {
        if (sign == 1) {
            $.loading(true);
            $.ajax({
                type: "GET",
                url: "/Elaborate/Evaluate/GetExpertAdd?name=" + nameOfExpert.val() + "&score=" + ExpertScore.val(),
                success: function (adata) {
                    clickTab27()
                    cancelTabe()
                    $.loading();
                    $.modalMsg("新增专家评分成功！", "success");
                }
            });
        } else if (sign == 2) {
            $.loading(true);
            $.ajax({
                type: "GET",
                url: "/Elaborate/Evaluate/GetExpertUpdate?id=" + giveScore + "&score=" + ExpertScore.val(),
                success: function (adata) {
                    clickTab27()
                    cancelTabe()
                    nameOfExpert.removeAttr("disabled").css("background-color", "#fff");
                    $.loading();
                    $.modalMsg("编辑成功！", "success");
                }
            });
        }
    }

