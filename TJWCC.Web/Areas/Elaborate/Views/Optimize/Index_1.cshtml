                                    scene.background = new Color(0xa0a0a0);
                                    scene.fog = new Fog(0xffffff, 200, 3000);
                                    scene.width = 1645;
                                    scene.height = 835;
                                    // // 显示三维坐标系 20表示长度 红x绿y蓝z
                                    @*var axes = new AxesHelper(1000);
                                    // // 添加坐标系到场景中
                                    scene.add(axes);*@

                                    //添加灯源
                                    var directionalLight1 = new DirectionalLight(0xffffff, 1.0);
                                    directionalLight1.position.set(164, 422, 473);
                                    var directionalLight2 = new DirectionalLight(0xffffff, 1.0);
                                    directionalLight2.position.set(-766, 328, -286);
                                    var directionalLight3 = new DirectionalLight(0xffffff, 1.0);
                                    directionalLight3.position.set(143, 345, -437);
                                    scene.add(directionalLight1);
                                    scene.add(directionalLight2);
                                    scene.add(directionalLight3);

                                    // scene.add( new THREE.CameraHelper( light.shadow.camera ) );

                                    // 地面
                                    var mesh = new Mesh(new PlaneBufferGeometry(10000, 10000), new MeshPhongMaterial({
                                        color: 0xffffff,
                                        depthWrite: false
                                    }));
                                    mesh.rotation.x = -Math.PI / 2;
                                    mesh.receiveShadow = true;
                                    scene.add(mesh);
                                    //地面网格
                                    @*var grid = new GridHelper(10000, 100, 0x000000, 0x000000);
                                    grid.material.opacity = 0.2;
                                    grid.material.transparent = true;
                                    scene.add(grid);*@


                                    //标签
                                    @*var beng1 = "1#泵";
                                    var beng1status = "开";
                                    var beng1Hz = "0.87";
                                    var beng2 = "2#泵";
                                    var beng2status = "开";
                                    var beng2Hz = "0.57";
                                    var beng3 = "3#泵";
                                    var beng3status = "关";
                                    var beng3Hz = "0.56";
                                    var yewei = 3.589;
                                    var spritey1 = makeTextSprite(beng1 + "\r\n" + beng1status + "\r\n" + beng1Hz + "Hz", {
                                        fontsize: 32,
                                        // fontface: "Georgia",
                                        borderColor: {
                                            r: 117,
                                            g: 117,
                                            b: 117,
                                            a: 1.0
                                        },
                                        backgroundColor: {
                                            r: 247,
                                            g: 253,
                                            b: 253,
                                            a: 1.0
                                        }
                                    });
                                    spritey1.position.set(110, 30, 175);
                                    scene.add(spritey1);
                                    var spritey2 = makeTextSprite(beng2 + "\r\n" + beng2status + "\r\n" + beng2Hz + "Hz", {
                                        fontsize: 32,
                                        // fontface: "Georgia",
                                        borderColor: {
                                            r: 117,
                                            g: 117,
                                            b: 117,
                                            a: 1.0
