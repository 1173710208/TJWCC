@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<style>
    .form .formTitle {
        position: relative;
        left: 0px;
        text-align: right;
        white-space: nowrap;
        font-weight: normal;
        width: 140px;
        padding-right: 15px;
    }
</style>
<script>
    var keyValue = $.request("keyValue");
    $(function () {
        initControl();
        //if (!!keyValue) {
        //    $.ajax({
        //        url: "/MeterManage/Meter/GetFormJson",
        //        data: { makerID: keyValue },
        //        dataType: "json",
        //        async: false,
        //        success: function (data) {
        //            $("#form1").formSerialize(data);
        //        }
        //    });
        //}
    });

    function submitForm() { 
        if (!$('#form1').formValid()) {
            return false;
        }
        $.submitForm({
            url: "/MeterManage/Meter/SetCommandSubmitForm",
            param: $("#form1").formSerialize(),
            success: function () {
                $.currentWindow().$("#gridList").trigger("reloadGrid");
            }
        })
    }
</script>
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <ul class="nav nav-tabs">
            <li role="presentation" class="active"><a href="#tab">设置下发指令</a></li>
            @*<li role="presentation"><a href="#tab">查询指令信息</a></li>*@
        </ul>
        <div id="tab" style="padding-top: 20px; margin-right: 30px;">
            <table class="form">

                <tr>
                    <th class="formTitle">IMEI</th>
                    <td class="formValue">
                        <input id="IMEI" name="IMEI" type="text" class="form-control  required" placeholder="请输入IMEI" />
                    </td>
                    <th class="formTitle">过流告警阈值(L/h)</th>
                    <td class="formValue">
                        <input id="OVERFLOW" name="OVERFLOW" type="text" class="form-control" placeholder="可选项" />
                    </td>
                </tr>
                <tr>
                <tr>
                    <th class="formTitle">持续过流时间(分钟)</th>
                    <td class="formValue">
                        <input id="OVERTIME" name="OVERTIME" type="text" class="form-control" placeholder="可选项" />
                    </td>
                    <th class="formTitle">反流告警阈值(L/h)</th>
                    <td class="formValue">
                        <input id="OPPOFLOW" name="OPPOFLOW" type="text" class="form-control" placeholder="可选项" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">持续反流告警时间</th>
                    <td class="formValue">
                        <input id="OPPOTIME" name="OPPOTIME" type="text" class="form-control" placeholder="可选项(分钟)" />
                    </td>
                    <th class="formTitle">加密使能</th>
                    <td class="formValue">
                        <input id="ISSEC" name="ISSEC" type="text" class="form-control" placeholder="可选项（1加密，0不加密）" />
                    </td>

                        @*<input id="ISSEC" name="ISSEC" type="checkbox"><label for="ISSEC">加密传输</label>*@
                    @*<th class="formTitle">既时告警ID</th>
                    <td class="formValue">
                        <input id="jsgjid" name="jsgjid" type="text" class="form-control" />
                    </td>*@
                </tr>
                @*<tr>
                    <th class="formTitle">服务器地址</th>
                    <td class="formValue">
                        <input id="ip" name="ip" type="text" class="form-control" />
                    </td>
                    <th class="formTitle">APN信息</th>
                    <td class="formValue">
                        <input id="apn" name="apn" type="text" class="form-control" />
                    </td>
                </tr>*@
                <tr>
                    <th class="formTitle">周期上报离散起始时间</th>
                    <td class="formValue">
                        <input id="CYCSTIME" name="CYCSTIME" type="text" class="form-control" placeholder="可选项（时间格式为HHMMSS）" />
                    </td>
                    <th class="formTitle">周期上报离散结束时间</th>
                    <td class="formValue">
                        <input id="CYCETIME" name="CYCETIME" type="text" class="form-control" placeholder="可选项（时间格式为HHMMSS）" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">周期上报离散估长</th>
                    <td class="formValue">
                        <input id="TIMELENGTH" name="TIMELENGTH" type="text" class="form-control" placeholder="可选项（默认10秒，1-6*10秒）" />
                    </td>
                    <th class="formTitle">周期上报频率(小时)</th>
                    <td class="formValue">
                        <input id="CYCRATE" name="CYCRATE" type="text" class="form-control" placeholder="可选项（默认24小时，可设1-24小时）" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">上报重发机制(0-4次)</th>
                    <td class="formValue">
                        <input id="RETRYTIMES" name="RETRYTIMES" type="text" class="form-control" placeholder="可选项（默认2次，可设0-4次）" />
                    </td>
                    <th class="formTitle">电池告警阈值(V)</th>
                    <td class="formValue">
                        <input id="VOLTALARM" name="VOLTALARM" type="text" class="form-control" placeholder="可选项（可设精度0.01V）" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">密集上报起始时间</th>
                    <td class="formValue">
                        <input id="ISTARTHOUR" name="ISTARTHOUR" type="text" class="form-control" placeholder="可选项（整小时）" />
                    </td>
                    <th class="formTitle">阀门开关指令(0关 1开)</th>
                    <td class="formValue">
                        <input id="VALVESTATUS" name="VALVESTATUS" type="text" class="form-control" placeholder="可选项（可设精度0.01V）" />
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>
